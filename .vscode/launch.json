    "version": "0.2.0",
    "configurations": [{
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/milestone-merger-console/bin/Debug/netcoreapp2.2/milestone-merger-console.dll",
            "args": ["--milestone:1.1.5"],
            "cwd": "${workspaceFolder}/src/milestone-merger-console",
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}